<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.homnet.panchonet.fragmentexample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FragmentExample"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".simplefragment.SimpleFragmentActivity"
            android:label="@string/title_activity_simple_fragment" >
        </activity>
        <activity
            android:name="org.homnet.panchonet.simplefragment.SimpleFragmentActivity"
            android:label="@string/title_activity_simple_fragment" >
        </activity>
        <activity
            android:name="org.homnet.panchonet.multiplefragment.MultipleFragmentActivity"
            android:label="@string/title_activity_multiple_fragment" >
        </activity>
    </application>

</manifest>
